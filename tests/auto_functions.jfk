int simple() {
    return 42;
}

int add(int a, int b) {
    return a + b;
}

float calculateAverage(int count, float sum) {
    return sum / count;
}

int recursiveFunction(int n) {
    if (n == 0) {
        return 0;
    }
    return n + recursiveFunction(n - 1);
}

print simple();

int sum;
sum = add(5, 7);
print sum;

float avg = calculateAverage(3, 15.0);
print avg;

int result = recursiveFunction(3);
print result;
